
<header>
      <nav>
        <a href = "/">Home</a>
        {{#if loggedIn}}
        <a href = "/dashboard">Dashboard</a>
        <button id = "logout" class = "btn-no-style">Log Out</button>
        {{else}}
        <a href = "/login">Login</a>
        {{/if}}
      </nav>
       <h1>
        <a href="/">The Tech Blog</a>
      </h1>
    </header>
    
<ul class="post-list">
  {{#each posts as |post|}}
  <li>
    {{> post-info post}}
  </li>
  {{/each}}
</ul>
